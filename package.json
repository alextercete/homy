{
  "name": "homy",
  "version": "0.1.0",
  "description": "Symlink your dotfiles across different operating systems",
  "bin": {
    "homy": "./bin/homy"
  },
  "scripts": {
    "lint": "jscs lib/ spec/ && jshint lib/ spec/",
    "test": "mocha --colors --recursive lib/ spec/",
    "watch:test": "npm test -- --reporter dot --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:alextercete/homy.git"
  },
  "keywords": [
    "dotfiles",
    "symlink",

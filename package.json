  "bugs": {
    "url": "https://github.com/alextercete/homy/issues"
  },
  "homepage": "https://github.com/alextercete/homy",
  "devDependencies": {
    "chai": "^3.1.0",
    "jscs": "^1.13.1",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "js-yaml": "^3.5.2",
    "lodash": "^4.2.0",
    "q": "^1.4.1",
    "q-io": "^1.13.1",
    "yargs": "^3.32.0"
  }
